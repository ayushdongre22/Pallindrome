#include <bits/stdc++.h>
using namespace std;
bool fun(string &s,int n){
    for(int i=0;i<n/2;i++){
        if(s[i]!=s[n-i-1]){return false;}
    }
    return true;
}
int main() {
    int n;
    cin>>n;
    string s;
    cin>>s;
    
    if(fun(s,n)==true){cout<<"Pallindrom";}
    else{cout<<"NOT pallindrome";}
    return 0;
}
